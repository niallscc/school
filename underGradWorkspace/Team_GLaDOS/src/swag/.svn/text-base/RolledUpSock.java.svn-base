package swag;

public class RolledUpSock extends Weapon  {
	/**
	 * Used for serialization
	 */
	private static final long serialVersionUID = 1L;

	public RolledUpSock() {
		this.imageFile="./ItemsIcons/RolledUpSock.png";
		this.itemName = "Rolled Up Sock";
		
		this.minDamage = 1;
		this.maxDamage = 2;
		this.strMultiplier = 0.05;
		this.cost = 1;
		this.vendorcost = this.cost*.5;
		this.buycost = this.cost*1.5;
		this.encumberanceAmount = 10;
		this.durability = 100;
		this.levelreq = 1;
	}

}

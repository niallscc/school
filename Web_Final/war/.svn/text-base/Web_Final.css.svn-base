	/** Image hovering stuff **/
	
.image-gallery {  
    
  margin-left:90px;
  margin-top:50px;
  padding-left:7%;
  padding-right:20px;
  padding-top:10px;
  padding-bottom:10px;  
}

figure { 
  padding-left:0px;
  padding-right:0px;
  padding-top:0px;
  padding-bottom:10px;  
  width: 600px; 
  height:300px;'
  background: white;
  /*border: 10px solid white; */
  -webkit-box-shadow: 0 3px 10px #ccc;
  -moz-box-shadow: 0 3px 10px #ccc;
  
  -webkit-transition: all 0.7s ease; 
  -moz-transition: all 1s ease;
  position: relative;
}

figcaption { 
  text-align: center; 
  display: block; 
  font-size: 12px; 
  font-style: italic; 
}

figure img { 
  width: 100%; /* Scale down */
}

figure:hover {
    
    outline: none;
    -webkit-transform: scale(1.5); 
    -moz-transform: scale(1.5);
    -webkit-box-shadow:0 3px 10px #666;
    -moz-box-shadow: 0 3px 10px #666;
    z-index: 9999;
}

figure:focus {
    outline: none;
    -webkit-transform: scale(2.2); 
    -moz-transform: scale(2.2);
    -webkit-box-shadow:0 3px 10px #666;
    -moz-box-shadow: 0 3px 10px #666;
    z-index: 9999;
}
	
	
	

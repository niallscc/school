/**This section is for the main menu */
	.demo-MenuBar
	{
		position:relative;
		/*top:100px;*/
		/*left:200px;*/
		font: bold 14px Georgia ;
		width: 100%;
		background: #b1e958;
		
		padding: 11px 0 8px 0; /* padding of the 4 sides of the menu */
		margin: 0;
		text-align: left; /* set value to "left", "center", or "right" to align menu accordingly */
	}
	.newUser-style{
	   color: white;
	}
	.demo-MenuBar table
	{
		
		font: bold 14px Georgia ;
		width: 100%;
		background: #b1e958;
		background:url(http://www.dndwebsitecreator.com/images/logo.png);
		background-size:300px 60px;
		background-position:90px -10px;
		background-repeat:no-repeat;
		 
		padding: 11px 0 8px 0; /* padding of the 4 sides of the menu */
		margin: 0;
		text-align: center; /* set value to "left", "center", or "right" to align menu accordingly */
	}
	.demo-MenuBar .gwt-MenuItem
	{
		display: inline;
		color:black;
		padding: 6px 8px 6px 8px; /* padding of the 4 sides of each menu link */
		margin-right: 15px; /* spacing between each menu link */
		text-decoration: none;
	}
	.demo-MenuBar .gwt-MenuItem-selected
	{
	    color: black;
	    background: url(data:image/svg+xml;base64,); /* IE9+ SVG equivalent  of linear gradients */
	    background: -moz-linear-gradient(top,  rgba(255,255,255,0.82) 0%, rgba(255,255,255,0.16) 100%); /* fade from white (0.82 opacty) to 0.16 opacity */
	    background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,rgba(255,255,255,0.82)), color-stop(100%,rgba(255,255,255,0.16)));
	    background: -webkit-linear-gradient(top,  rgba(255,255,255,0.82) 0%,rgba(255,255,255,0.16) 100%);
	    background: -o-linear-gradient(top,  rgba(255,255,255,0.82) 0%,rgba(255,255,255,0.16) 100%);
	    background: -ms-linear-gradient(top,  rgba(255,255,255,0.82) 0%,rgba(255,255,255,0.16) 100%);
	    background: linear-gradient(top,  rgba(255,255,255,0.82) 0%,rgba(255,255,255,0.16) 100%);
	    filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#d1ffffff', endColorstr='#29ffffff',GradientType=0 );
	    -moz-box-shadow: 0 0 5px #595959; /* CSS3 box shadows */
	    -webkit-box-shadow: 0 0 5px #595959;
	    box-shadow: 0 0 5px #595959;
	    padding-top: 12px; /* large padding to get menu item to protrude upwards */
	    padding-bottom: 20px; /* large padding to get menu item to protrude downwards */
}


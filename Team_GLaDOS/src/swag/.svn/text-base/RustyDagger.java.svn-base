package swag;

public class RustyDagger extends Weapon  {
	/**
	 * Used for serialization
	 */
	private static final long serialVersionUID = 1L;

	public RustyDagger(){
		this.imageFile="./ItemsIcons/RustyDagger.png";
		this.itemName="Rusty Dagger";
		this.minDamage = 2;
		this.maxDamage = 4;
		this.strMultiplier = 0.1;
		this.cost = 5;
		this.vendorcost = this.cost*.5;
		this.buycost = this.cost*1.5;
		this.encumberanceAmount = 20;
		this.durability = 100;
		this.levelreq = 1;
	}


}
